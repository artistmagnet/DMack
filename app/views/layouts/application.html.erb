<!DOCTYPE html>
<html>
<head>
  <title>Artist Magnet</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <p class="notice" style="display: none"><%= notice %></p>
  <p class="alert" style="display: none"><%= alert %></p>
  <!--<h3>< %= link_to 'Artist magnet', root_path, style: "text-decoration: none" %> <br /></h3>-->

  <!-- Page container -->
  <div class="page-container">
    <!-- Sidebar -->
    <aside id="sidebar">
      <!-- Sidebar header -->
      <header class="sidebar-header">
        <h1><img src="/images/logo.png" alt="Artist Magnet"></h1>
        <a href="" id="make-compact"><span class="typcn typcn-th-menu"></span></a>
        <a href="#" id="hide-menu"><span class="fa fa-caret-square-o-left"></span></a>
      </header>
      <!-- /Sidebar header -->
      <!-- Sidebar group -->
      <ul class="sidebar-group">
        <br />
        <li class="sidebar-group-header">ArtistSpace</li>
        <li class="has-submenu">
          <a href="#"><i class="typcn typcn-tabs-outline"></i> <span>Resumes</span></a>
          <div class="sidebar-link-addon"><i class="typcn typcn-plus"></i></div>
          <!-- Sidebar submenu -->
          <ul class="sidebar-submenu">
            <li><a href="#"><i class="typcn typcn-starburst text-red"></i><span> Performing Artist</span></a></li>
            <li><a href="#"><i class="typcn typcn-starburst text-blue"></i><span> Director</span></a></li>
            <li><a href="#"><i class="typcn typcn-starburst text-green"></i><span> Crew Member</span></a></li>
            <li><a href="#"><span><em>Build a Resume</em></span></a></li>
          </ul>
          <!-- /Sidebar submenu -->
        </li>
        <li>
          <a href="#"><i class="typcn typcn-group"></i> <span>Add friends</span></a>
        </li>
        <li>
          <a href="#"><i class="typcn typcn-star-outline"></i> <span>Upgrade your account</span></a>
        </li>
        <li class="has-submenu">
          <a href="#"><i class="typcn typcn-cog"></i> <span>Other</span></a>
          <div class="sidebar-link-addon"><i class="typcn typcn-plus"></i></div>
          <!-- Sidebar submenu -->
          <ul class="sidebar-submenu">
            <li><a href="blank.html"><i class="typcn typcn-starburst text-red"></i><span> Account Settings</span></a></li>
            <li><a href="blank.html"><i class="typcn typcn-starburst text-blue"></i><span> Log out</span></a></li>
          </ul>
          <!-- /Sidebar submenu -->
        </li>
      </ul>
      <!-- /Sidebar group -->

      <!-- /Sidebar panel -->
    </aside>
    <!-- /Sidebar -->

    <!-- Content -->
    <div id="content" style="min-height: 873px;">

      <%= yield %>

    </div>

  </div>
  <!-- /Page container -->

</body>
</html>
