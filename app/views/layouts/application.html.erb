<!DOCTYPE html>
<html>
<head>
  <title>Artist Magnet</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <link type="text/css" rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800,400italic%7cMontserrat:400,700">
   <link rel="stylesheet" href="//code.jquery.com/ui/1.11.2/themes/smoothness/jquery-ui.css">
  <%= csrf_meta_tags %>
</head>
<body>

  <p class="notice" style="display: none"><%= notice %></p>
  <p class="alert" style="display: none"><%= alert %></p>

  <!-- Page container -->
  <div class="page-container" id="main-container">
    <% if user_signed_in? %>
    <%= render 'layouts/includes/sidebar' %>
    <%else%>
    <%= render 'layouts/includes/sidebar' %>
    <%end%>
    <!-- Content -->
    <div id="content" style="min-height: 873px;">
      <div class="container-fluid p-top-20">
        <div class="row">
          <div class="col-md-12">
            <div class="flash-message">
              <%= render 'layouts/includes/notice' %>
            </div>
            <%= yield %>
          </div>
        </div>
      </div>
    </div>

  </div>
  <!-- /Page container -->
 <%= render "layouts/footer"%>

</body>
</html>
