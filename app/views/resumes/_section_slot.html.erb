<div class="row" id="section_slot_<%= section_slot.id %>">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <h4>
              <span><%= section_slot.id %></span>
            </h4>
            <br />
            <p>
              <% table = section_slot.section %>
              <%= table.columns %>
              <%= table.columns[0] %>
              <%= table.columns[1] %>
            </p>
          </div>
        </div>
      </div>
      <div class="panel-body">
        <div class="row m-bottom-20">
          <div class="col-md-12">
            <i class="fa fa-arrows pull-right text-pale top-right-handle"></i>
            <h4>
              <span><%= section_slot.section.title %></span>
              <%= link_to edit_polymorphic_path(section_slot.section) do %>
              <!--< %= link_to edit_education_table_path(section_slot.section) do %>-->
                <button type="button" class="btn btn-primary pull-right m-top-m6">Edit</button>
              <% end %>
            </h4>
            <br>
            <div class="row">
              <div class="col-md-2">
                <strong><%= table.columns[table.positions[0]-1] %></strong>
              </div>
              <div class="col-md-2">
                <strong><%= table.columns[table.positions[1]-1] %></strong>
              </div>
              <div class="col-md-2">
                <strong><%= table.columns[table.positions[2]-1] %></strong>
              </div>
              <div class="col-md-2">
                <strong><%= table.columns[table.positions[3]-1] %></strong>
              </div>
              <div class="col-md-2">
                <strong><%= table.columns[table.positions[4]-1] %></strong>
              </div>
              <div class="col-md-2">
                <strong><%= table.columns[table.positions[5]-1] %></strong>
              </div>
            </div>
            <% table.rtable_items.each do |line| %>
              <div class="h-movable row">
                <div class="col-md-2">
                  <%= line.values[0] %>
                </div>
                <div class="col-md-2">
                  <%= line.values[1] %>
                </div>
                <div class="col-md-2">
                  <%= line.values[2] %>
                </div>
                <div class="col-md-2">
                  <%= line.values[3] %>
                </div>
                <div class="col-md-2">
                  <%= line.values[4] %>
                </div>
                <div class="col-md-2">
                  <%= line.values[5] %>
                </div>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

