<%#education(cust,"custom")%>
<tr class="ui-state-disabled custom_row" id="bold_col">
  <td class="col-sm-2 drag-enable">
    
     <%= text_field_tag 'resume[customs_attributes][0][ccolumns][]',"",:class=>"form-control diminished-font custom_cols customs col1",:placeholder=>"Enter Custom Title",:id=>"1"%> 
  </td>
  <td class="col-sm-2 drag-enable">
    <%= text_field_tag 'resume[customs_attributes][0][ccolumns][]',"",:class=>"form-control diminished-font custom_cols customs col2",:placeholder=>"Enter Custom Title",:id=>"2"%> 
  </td>
  <td class="col-sm-2 drag-enable">
    <%= text_field_tag 'resume[customs_attributes][0][ccolumns][]',"",:class=>"form-control diminished-font custom_cols customs col3",:placeholder=>"Enter Custom Title",:id=>"3"%> 
  </td>
  <td class="col-sm-2 drag-enable">
   <%= text_field_tag 'resume[customs_attributes][0][ccolumns][]',"",:class=>"form-control diminished-font custom_cols customs col4",:placeholder=>"Enter Custom Title",:id=>"4"%> 
  </td>

  <td class="col-sm-1">
    <a class="btn btn-default remove_title_row" title="Remove Title Row" data-association="customs" data-success="true" data-confirm='Are you sure?' href="javascript:void(0)">-</a>
    <%=hidden_field_tag 'resume[customs_attributes][0][is_bold]',true,{:class=>"tr_bold"}%>
  </td>
</tr>
