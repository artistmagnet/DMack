<%education(e)%>
 <!-- <div class="form-group"> -->

    <!-- <div class="col-sm-1"> 
        <img width="30px;" alt="Icon" src="/assets/drag.jpg">
      </div> -->
  <tr class="">    
    <td class="col-sm-2 drag-enable">      
      <input class="form-control diminished-font educations col1" name="school" value="<%= education(e).present? ? !education(e)[1].blank? ? education(e)[1] : "" : ""%>" placeholder="Institution" type="text" id="1">   
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations col2" name="city" value="<%= education(e).present? ? !education(e)[2].blank? ? education(e)[2] : ""  : "" %>" placeholder="City" type="text" id="2">
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations col3" name="state" value="<%= education(e).present? ? !education(e)[3].blank? ? education(e)[3] : "" : "" %>" placeholder="State/Region" type="text" id="3">
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations col4" name="country" value="<%= education(e).present? ? !education(e)[4].blank? ? education(e)[4] : "" : "" %>" placeholder="Country" type="text" id="4">
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations col5" name="degree" value="<%= education(e).present? ? !education(e)[5].blank? ? education(e)[5] : "" : "" %>" placeholder="Degree/Concentration" type="text" id="5">
    </td>
    <td class="col-sm-1 drag-enable">
      <input class="form-control diminished-font educations col6" name="year" value="<%= education(e).present?  ? !education(e)[6].blank? ? education(e)[6] : "" : "" %>" placeholder="Year" type="text" id="6">
    </td>
    <td class="col-sm-1 drag-enable">
      <a class="btn btn-default remove_row" title="Remove Row" data-association="educations" data-success="true" href="javascript:void(0)" data-confirm='Are you sure?'>-</a>
    </td>
   </tr> 
    
    
<!-- </div> -->
