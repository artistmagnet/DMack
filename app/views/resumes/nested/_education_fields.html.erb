<%#education(e)%>
 <!-- <div class="form-group"> -->

    <!-- <div class="col-sm-1"> 
        <img width="30px;" alt="Icon" src="/assets/drag.jpg">
      </div> -->
  <tr class="">    
    <td class="col-sm-2 drag-enable">      
      <input class="form-control diminished-font educations education col1" name="resume[educations_attributes][0][ecolumns][]" value="<%#= education(e).present? ? !education(e)[1].blank? ? education(e)[1] : "" : ""%>" placeholder="Institution" type="text" id="1">
      <%#=hidden_field_tag 'resume[rtables_attributes][0][positions][]'%>   
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations education col2" name="resume[educations_attributes][0][ecolumns][]" value="<%#= education(e).present? ? !education(e)[2].blank? ? education(e)[2] : ""  : "" %>" placeholder="City" type="text" id="2">
      <%#=hidden_field_tag 'resume[rtables_attributes][0][positions][]'%>
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations education col3" name="resume[educations_attributes][0][ecolumns][]" value="<%#= education(e).present? ? !education(e)[3].blank? ? education(e)[3] : "" : "" %>" placeholder="State/Region" type="text" id="3">
      <%#=hidden_field_tag 'resume[rtables_attributes][0][positions][]'%>
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations education col4" name="resume[educations_attributes][0][ecolumns][]" value="<%#= education(e).present? ? !education(e)[4].blank? ? education(e)[4] : "" : "" %>" placeholder="Country" type="text" id="4">
      <%#=hidden_field_tag 'resume[rtables_attributes][0][positions][]'%>
    </td>
    <td class="col-sm-2 drag-enable">
      <input class="form-control diminished-font educations education col5" name="resume[educations_attributes][0][ecolumns][]" value="<%#= education(e).present? ? !education(e)[5].blank? ? education(e)[5] : "" : "" %>" placeholder="Degree/Concentration" type="text" id="5">
      <%#=hidden_field_tag 'resume[rtables_attributes][0][positions][]'%>
    </td>
    <td class="col-sm-1 drag-enable">
      <input class="form-control diminished-font educations education col6" name="resume[educations_attributes][0][ecolumns][]" value="<%#= education(e).present?  ? !education(e)[6].blank? ? education(e)[6] : "" : "" %>" placeholder="Year" type="text" id="6">
      
    </td>
    <td class="col-sm-1 drag-enable">
      <a class="btn btn-default remove_row" title="Remove Row" data-association="educations" data-success="true" href="javascript:void(0)" data-confirm='Are you sure?'>-</a>
      <%=hidden_field_tag 'resume[educations_attributes][0][is_bold]',"",:class=>"tr_bold"%>
    </td>
   </tr> 
    
    
