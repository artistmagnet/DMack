<%education(e)%>
<div class="fields">
 <div class="form-group">
    <!-- <div class="col-sm-1"> 
        <img width="30px;" alt="Icon" src="/assets/drag.jpg">
      </div> -->
    <div class="col-sm-2">      
      <input class="form-control diminished-font education col1" name="school" value="<%= education(e).present? ? !education(e)[1].blank? ? education(e)[1] : "Institution" : "Institution"%>" placeholder="Institution" type="text", id="1">   
    </div>
    <div class="col-sm-2">
      <input class="form-control diminished-font education col2" name="city" value="<%= education(e).present? ? !education(e)[2].blank? ? education(e)[2] : "City"  : "City" %>" placeholder="City" type="text" id="2">
    </div>
    <div class="col-sm-2">
      <input class="form-control diminished-font education col3" name="state" value="<%= education(e).present? ? !education(e)[3].blank? ? education(e)[3] : "State/Region" : "State/Region" %>" placeholder="State/Country" type="text" id="3">
    </div>
    <div class="col-sm-2">
      <input class="form-control diminished-font education col4" name="country" value="<%= education(e).present? ? !education(e)[4].blank? ? education(e)[4] : "Country" : "Country" %>" placeholder="Zip Code" type="text" id="4">
    </div>
    <div class="col-sm-2">
      <input class="form-control diminished-font education col5" name="degree" value="<%= education(e).present? ? !education(e)[5].blank? ? education(e)[5] : "Degree/Concentration" : "Degree/Concentration" %>" placeholder="Degree" type="text" id="5">
    </div>
    <div class="col-sm-1">
      <input class="form-control diminished-font education col6" name="year" value="<%= education(e).present?  ? !education(e)[6].blank? ? education(e)[6] : "Year" : "Year" %>" placeholder="Year"type="text" id="6">
    </div>
    <div class="col-sm-1">
      <a class="btn btn-default remove_nested_fields remove_columns" data-association="educations" data-success="true" href="javascript:void(0)">-</a>
    </div>
    
  </div>
</div>
