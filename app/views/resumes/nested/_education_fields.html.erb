<%education(e)%>
<div class="fields">
 <div class="form-group ">
    <div class="col-sm-2">      
      <input class="form-control education col1" name="school" value="<%= education(e).present? ? !education(e)[1].blank? ? education(e)[1] : "Institution" : "Institution"%>" placeholder="Institution" type="text", id="1">   
    </div>
    <div class="col-sm-2">
      <input class="form-control education col2" name="city" value="<%= education(e).present? ? !education(e)[2].blank? ? education(e)[2] : "City"  : "City" %>" placeholder="City" type="text" id="2">
    </div>
    <div class="col-sm-2">
      <input class="form-control education col3" name="state" value="<%= education(e).present? ? !education(e)[3].blank? ? education(e)[3] : "State/Country" : "State/Country" %>" placeholder="State/Country" type="text" id="3">
    </div>
    <div class="col-sm-2">
      <input class="form-control education col4" name="country" value="<%= education(e).present? ? !education(e)[4].blank? ? education(e)[4] : "Zip Code" : "Zip Code" %>" placeholder="Zip Code" type="text" id="4">
    </div>
    <div class="col-sm-2">
      <input class="form-control education col5" name="degree" value="<%= education(e).present? ? !education(e)[5].blank? ? education(e)[5] : "Degree" : "Degree" %>" placeholder="Degree" type="text" id="5">
    </div>
    <div class="col-sm-1">
      <input class="form-control education col6" name="year" value="<%= education(e).present?  ? !education(e)[6].blank? ? education(e)[6] : "Year" : "Year" %>" placeholder="Year" style="width: 59px;" type="text" id="6">
    </div>
    <div class="col-sm-1">
      <a class="btn btn-default remove_nested_fields remove_columns" data-association="educations" data-success="true" href="javascript:void(0)">Remove</a>
    </div>
  </div>
</div>
