<div id="container">
<a class="btn btn-default remove_nested_field remove_education pull-right" data-association="Other" data-success="true" href="javascript:void(0)" title="Remove Section">X</a>
  <%=f.fields_for :resume_sections, sec do |o|%> 
    <%= o.text_field :section_name,:value=>sec.section_name,:class=>"resume_section section_heading"%>
    <%= o.hidden_field :position,:class=>"resume_section section_heading"%>
    <%= o.hidden_field :section_id,:value=>5,:class=>"resume_section section_heading"%>
  <%end%> 
</div>

