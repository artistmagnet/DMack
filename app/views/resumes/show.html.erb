<div class="container-fluid m-top-20">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-body" id="show-resume">         
          <section class="list-resumes">            
            <div class="row">
                
              <div class="col-md-3 resume_attribute">
               
                <% if @resume.resume_attribute.try(:age) %>
                <span><strong>Age: <%=@resume.resume_attribute.age%> </strong></span><br/>
                <%end%>
                <% if @resume.resume_attribute.try(:height) %>
                <span><strong>Height: <%=@resume.resume_attribute.height%> </strong></span><br/>
                <%end%>
                <% if @resume.resume_attribute.try(:weight) %>
                 <span><strong>Weight: <%=@resume.resume_attribute.weight%>  </strong></span><br/>
                 <%end%>
                  <% if @resume.resume_attribute.try(:hair_color) %>
                 <span><strong>Hair Color: <%=@resume.resume_attribute.hair_color.capitalize%>  </strong></span><br/>
                 <%end%>
                 <% if @resume.resume_attribute.try(:hair_lenght) %>
                 <span><strong>Hair Length: <%=@resume.resume_attribute.hair_lenght%></strong>  </span><br/>
                 <% end %>
                 <% if @resume.resume_attribute.try(:eye_color) %>
                 <span><strong>Eyes: <%=@resume.resume_attribute.eye_color.capitalize%></strong>  </span><br/>
                 <%end%>
                 <% if @resume.resume_attribute.try(:vocal_range) %>
                 <span><strong>Voice: <%=@resume.resume_attribute.vocal_range.capitalize%></strong>  </span><br/>
                 <%end%>
                 <%if @resume.resume_attribute.try(:ethnicity) %>
                 <span><strong>Ethnicity: <%=@resume.resume_attribute.ethnicity.capitalize%></strong>  </span><br/>
                 <%end%>
               </div>

               
              <div class="col-md-4 contact_block">
                 <h2><%=@resume.contact_info.nick_name.upcase%></h2>
                 <h4><%=@resume.try(:performer_type).upcase%></h4>
                 <h5><%=@resume.try(:union_guild).upcase%></h5>
                 
                 <br/>
                 <span><strong><%=@resume.contact_info.try(:email)%> </strong></span><br/>
               
                <span><strong><%=@resume.contact_info.try(:facebook)%></strong></span><br/>
                <span><strong><%=@resume.contact_info.try(:twitter)%></strong></span><br/>
                <span><strong><%=@resume.contact_info.try(:linkedin)%></strong></span><br/>
                <span><strong><%=@resume.contact_info.try(:address)%></strong></span><br/>
                 <span><strong><%=@resume.contact_info.try(:phone1)%> </strong></span><br/>
               </div>

                <div class="col-md-4">
                 <%= image_tag @resume.image.try(:url),:style=>"width: 250px;"%>
               </div>
              
                
             </div>
                   
            </section>
            <br/> 
            <!-- <hr/> -->
             <section class="list-resumes">
                <h3>Representation:</h3>
              
                     <% @resume.representations.each do |representation|%>
                      <div class="row">
                        <div class="col-md-2"><%= representation.company %></div>
                        <div class="col-md-2"><%= representation.contact_name %></div>
                        <div class="col-md-2"><%= representation.title  %></div>
                        <div class="col-md-2"><%= representation.phone  %></div>
                        <div class="col-md-2"><%= representation.address  %></div>
                      </div>
                      <%end%>                 
              </section><br />



            <section class="list-resumes">
              <h3>THEATRE</h3>
             
                <% @resume.roles.each do |theatre|%>
                <div class="row">
                  <div class="col-md-3"><%= link_to theatre.production_name.upcase,theatre.production ,:target=>"_blank" if !theatre.production.blank?%></div>
                  <div class="col-md-3"><%= theatre.name.capitalize%></div>
                  <div class="col-md-3"><%= theatre.director_name.capitalize%></div>
                  <div class="col-md-3"><%= get_venue_and_company(theatre) %></div>
                </div>
                <%end%>
              
            </section ><br/>
            
            <section class="list-resumes">
              <h3>TRAINING</h3>
              
                   <% @resume.educations.each do |education|%>
                    <div class="row">
                      <div class="col-md-2"><%= education.school.capitalize %></div>
                      <div class="col-md-2"><%= education.city.capitalize %></div>
                      <div class="col-md-2"><%= education.state.capitalize  %></div>
                      <div class="col-md-2"><%= education.country.capitalize  %></div>
                      <div class="col-md-2"><%= education.degree.capitalize  %></div>
                      <div class="col-md-2"><%= education.year%></div>
                    </div> 
                    <%end%>                 
            </section><br/>
     
