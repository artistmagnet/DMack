              <section class="list-resumes">
                <h3>Representation:</h3>
                <div data-example-id="striped-table" class="bs-example">
                  <table class="table table-striped">
                    <thead>
                      <tr>
                        <th>Company</th>
                        <th>Contact_name</th>
                        <th>Title</th>
                        <th>Phone</th>
                        <th>Adrress</th>
                      </tr>
                    </thead>
                    <tbody>
                     <% @resume.representations.each do |representation|%>
                      <tr>
                        <td><%= representation.company %></td>
                        <td><%= representation.contact_name %></td>
                        <td><%= representation.title  %></td>
                        <td><%= representation.phone  %></td>
                        <td><%= representation.address  %></td>
                      </tr> 
                      <%end%>                 
                    </tbody>
                  </table>
                </div>
              </section><br />

              <section class="list-resumes">
                <h3>Skills:</h3>
                <div data-example-id="striped-table" class="bs-example">
                  <table class="table table-striped">
                    <thead>
                      <tr>
                        <th>Category</th>
                        <th>Skills</th>
                      </tr>
                    </thead>
                    <tbody>
                     <% @resume.skills.each do |skill|%>
                      <tr>
                        <td><%= skill.category.try(:name) %></td>
                        <td><li style="list-style-type: none;"><%= skill.skills  if !skill.skills.blank?%></li></td>
                      </tr> 
                      <%end%>                 
                    </tbody>
                  </table>
                </div>
              </section><br />

              <section class="list-resumes">
              <h3>Custom:</h3>
              <div data-example-id="striped-table" class="bs-example">
                <table class="table table-striped">            
                  <tbody>
                  <%=render 'resumes/nested/final_education_fields',:e=>@resume,:cust=>"custom"%>
                   <% @resume.customs.each do |custom|%>
                    <tr>
                      <td><%= custom.custom1 %></td>
                      <td><%= custom.custom2 %></td>
                      <td><%= custom.custom3  %></td>
                      <td><%= custom.custom4  %></td>
                      <td><%= custom.custom5  %></td>
                      <td><%= custom.custom6  %></td>
                    </tr> 
                    <%end%>                 
                  </tbody>
                </table>
              </div>
            </section><br/>

            <section class="list-resumes">
              <h3>Other:</h3>
              <div data-example-id="striped-table" class="bs-example">
                <table class="table table-striped">                  
                  <tbody>
                   <% @resume.others.each do |other|%>
                    <tr>
                      <td><%= other.content %></td>
                    </tr> 
                    <%end%>
                  </tbody>
                </table>
              </div>
            </section><br />

            </div>
            
          </div>

        </div>
      </div>
    </div>
  </div>
</div>

<div class="container-fluid" id="section_slot" data-update-url="<%= sort_section_slots_url %>">
  <%# @resume.section_slots.each do |section_slot| %>
    <%#= render partial: 'section_slot', locals: {section_slot: section_slot} %>
  <%# end %>
</div>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <!--<button type="button" class="btn btn-primary m-top-m6">Add a skills section</button>-->
      <%#= link_to add_resume_table_path(@resume), method: :post do %>
        <!-- <button type="button" class="btn btn-primary m-top-m6">Add a custom section</button> -->
      <%# end %>
    </div>
  </div>
</div>
