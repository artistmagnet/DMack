              <section class="list-resumes">
                <h3>SPECIAL SKILLS</h3>
                <div data-example-id="striped-table" class="bs-example">
                <p>
                    
                       <%#= skill.category.try(:name) %>
                        <li style="list-style-type: none;"><%= @resume.skills.map { |tag| tag.skills.capitalize }.join(', ')  if !@resume.skills.blank?%></li>
                     
                </p>
                </div>
              </section><br />

              <section class="list-resumes">
              <h3>Custom:</h3>
             
                  <%#=render 'resumes/nested/final_education_fields',:e=>@resume,:cust=>"custom"%>
                   <% @resume.customs.each do |custom|%>
                    <div class="row">
                      <div class="col-md-2"><%= custom.custom1 %></div>
                      <div class="col-md-2"><%= custom.custom2 %></div>
                      <div class="col-md-2"><%= custom.custom3  %></div>
                      <div class="col-md-2"><%= custom.custom4  %></div>
                      <div class="col-md-2"><%= custom.custom5  %></div>
                      <div class="col-md-2"><%= custom.custom6  %></div>
                    </div> 
                    <%end%>
            </section><br/>

            <section class="list-resumes">
              <h3>Other:</h3>
           
            <li style="list-style-type: none;"><%= @resume.others.map { |tag| tag.content.capitalize }.join(', ')  if !@resume.others.blank?%></li>
            </section><br />

            </div>
            
          </div>

        </div>
      </div>
    </div>
  </div>
</div>

<div class="container-fluid" id="section_slot" data-update-url="<%= sort_section_slots_url %>">
  <%# @resume.section_slots.each do |section_slot| %>
    <%#= render partial: 'section_slot', locals: {section_slot: section_slot} %>
  <%# end %>
</div>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <!--<button type="button" class="btn btn-primary m-top-m6">Add a skills section</button>-->
      <%#= link_to add_resume_table_path(@resume), method: :post do %>
        <!-- <button type="button" class="btn btn-primary m-top-m6">Add a custom section</button> -->
      <%# end %>
    </div>
  </div>
</div>
