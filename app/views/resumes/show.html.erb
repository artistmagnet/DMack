<div class="container-fluid m-top-20">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-body" id="show-resume">         
          <section class="list-resumes">            
            <div class="row">
               <div class="col-md-8">
                 <h4> <%=@resume.contact_info.name%></h4>
                 <span><%=@resume.contact_info.address%>  </span><br/>
                 <span>mobile: <%=@resume.contact_info.phone1%>  </span><br/>
                 <span>e-mail: <%=@resume.contact_info.email%>  </span><br/>
                 <span><%=@resume.contact_info.try(:facebook)%> , <%=@resume.contact_info.try(:twitter)%>, <%=@resume.contact_info.try(:linkedin)%> </span><br/>
               </div>
               <div class="col-md-4">
                 <%= image_tag @resume.image.try(:url),:style=>"width: 250px;"%>
               </div>
              </div>       
            </section>
            <br/> <hr/>
            <section class="list-resumes">
              <h3>Theatre:</h3>
              <div data-example-id="striped-table" >
                <table class="table table-striped">
                  <tbody>
                    <tr>
                      <th>Production</th>
                      <th>Venue</th>
                      <th>Role</th>
                      <th>Company</th>
                      <th>Directed By</th>
                      <th>Date of Performance</th>
                      <th>Location</th>                      
                    </tr>
                   <% @resume.theatres.each do |theatre|%>
                    <tr>
                      <td><h4><%= link_to theatre.production,production_path(Production.find_by(:name=>theatre.production).id) ,:target=>"_blank" if !theatre.production.blank?%></h4></td>
                      <td><h4><%=link_to theatre.venue,venue_path(Venue.find_by(:name=>theatre.venue).id) ,:target=>"_blank" if !theatre.venue.blank? %></td>
                      <td><%= theatre.role  %></h4></td>
                      <td><h4><%=link_to theatre.company,company_path(Company.find_by(:name=>theatre.company).id) ,:target=>"_blank" if !theatre.company.blank?%></h4></td>
                      <td><%= theatre.directed_by  %></td>
                      <td><%= theatre.performance_date  %></td>
                      <td><%= theatre.location  %></td>
                    </tr> 
                    <%end%>                 
                  </tbody>
                </table>
              </div>
            </section ><br/>
            
            <section class="list-resumes">
              <h3>Education/Training:</h3>
              <div data-example-id="striped-table" class="bs-example">
                <table class="table table-striped">            
                  <tbody>
                  <%=render 'resumes/nested/final_education_fields',:e=>@resume,:cust=>""%>
                   <% @resume.educations.each do |education|%>
                    <tr>
                      <td><%= education.school %></td>
                      <td><%= education.city %></td>
                      <td><%= education.state  %></td>
                      <td><%= education.country  %></td>
                      <td><%= education.degree  %></td>
                      <td><%= education.year  %></td>
                    </tr> 
                    <%end%>                 
                  </tbody>
                </table>
              </div>
            </section><br/>
     
            <%if @resume.resume_type== "Performing Artist"%>
              <section class="list-resumes">
                <h3>Attributes:</h3>
                  <div data-example-id="striped-table" class="bs-example">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th>Gender</th>
                          <th>Age</th>
                          <th>Height</th>
                          <th>Weight</th>
                          <th>Hair color</th>
                          <th>Hair Lenght</th>
                          <th>Eye color</th>
                          <th>Vocal Range</th>
                          <th>Ethnicity</th>

                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td><%= @resume.try(:resume_attribute).try(:gender) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:age) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:height) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:weight) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:hair_color) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:hair_lenght) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:eye_color) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:vocal_range) %></td>
                          <td><%= @resume.try(:resume_attribute).try(:ethnicity) %></td>
                        </tr>                  
                      </tbody>
                    </table>
                  </div> 
                </section>
              <%end%><br/>
            
